<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="TaskPaneApp">
  <Id>bd11f767-d886-4712-abc9-3deb9d8f810a</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>OfficeDev</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="ServerAuth sample"/>
  <Description DefaultValue="Office Add-in ServerAuth sample for NodeJS"/>
  <SupportUrl DefaultValue="https://github.com/officedev/office-add-in-nodejs-serverauth/issues"/>
  <Hosts>
    <Host Name="Document"/>
    <Host Name="Workbook"/>
    <Host Name="Presentation"/>
    <Host Name="Project"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://localhost:3000"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <AppDomains>
    <AppDomain>https://localhost:3000</AppDomain>
    <AppDomain>https://localhost:3001</AppDomain>
  </AppDomains>
</OfficeApp>
